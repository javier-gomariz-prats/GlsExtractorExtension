{
    "manifest_version": 3,
    "name": "GLSdataExtractor",
    "description": "GLSdataExtractor for SendingBay.",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png"
      }
    },
    "host_permissions": [
        "http://localhost:3000/*"
    ],

    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ]
  }
  